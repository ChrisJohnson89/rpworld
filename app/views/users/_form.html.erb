<%= form_for @user do |f|%>
    <%= f.label :displayname%>
    <%= f.text_field :displayname%>

    <%= f.label :password%>
    <%= f.password_field :password%>

    <%= f.label :password_confirmation%>
    <%= f.password_field :password_confirmation%>

    <%= f.label :email%>
    <%= f.text_field :email%>
    <%= f.submit%>
<%end%>
